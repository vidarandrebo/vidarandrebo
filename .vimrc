set number
set relativenumber
set tabstop=4
set softtabstop=4
set shiftwidth=4
set expandtab
set autoindent
set fileformat=unix
set encoding=utf-8
set wrap linebreak
syntax enable
filetype indent plugin on
inoremap øø {
inoremap ææ }
inoremap ØØ [
inoremap ÆÆ ]
inoremap åå {}
inoremap ÅÅ ()
nnoremap JJ <C-W>j
nnoremap KK <C-W>k
nnoremap HH <C-W>h
nnoremap LL <C-W>l
nnoremap == <C-W>=
nnoremap <silent> ^ :resize +2<CR>
nnoremap <silent> ** :resize -2<CR>
nnoremap << 5<C-w><
nnoremap >> 5<C-w>>
